#include<stdio.h>#include<math.h>int clatulate(int n)//透过平方来寻找高度{    for (int i = 1; i <= n;i++){        if(pow(i,2)>n){            return i-1;        }        if(pow(i,2)==n){            return i;        }    }    return 0;}void goldprint(int high,char c)//打印金字塔{    int j;    for (int i = 1; i <= high;i++)    {        for (j=0; j < high-i;j++)            printf(" ");        for (j=0; j < (2*i-1);j++)            printf("%c",c);        printf("\n");    }}int main(){    int n,high;    char c;    scanf("%d", &n);    getchar();    c=getchar();    high = clatulate(n);    goldprint(high, c);    printf("%d", (int)(n-pow(high,2)));}#include<stdio.h>#include<math.h>intclatulate(intn)//透过平方来寻找高度{for(inti=1;i<=n;i++){if(pow(i,2)>n){returni-1;}if(pow(i,2)==n){returni;}}return0;}voidgoldprint(inthigh,charc)//打印金字塔{intj;for(inti=1;i<=high;i++){for(j=0;j<high-i;j++)printf("");for(j=0;j<(2*i-1);j++)printf("%c",c);printf("\n");}}intmain(){intn,high;charc;scanf("%d",&n);getchar();c=getchar();high=clatulate(n);goldprint(high,c);printf("%d",(int)(n-pow(high,2)));}#include<stdio.h>#include<math.h>intclatulate(intn)//透过平方来寻找高度{for(inti=1;i<=n;i++){if(pow(i,2)>n){returni-1;}if(pow(i,2)==n){returni;}}return0;}voidgoldprint(inthigh,charc)//打印金字塔{intj;for(inti=1;i<=high;i++){for(j=0;j<high-i;j++)printf("");for(j=0;j<(2*i-1);j++)printf("%c",c);printf("\n");}}intmain(){intn,high;charc;scanf("%d",&n);getchar();c=getchar();high=clatulate(n);goldprint(high,c);printf("%d",(int)(n-pow(high,2)));}#include<stdio.h>#include<math.h>intclatulate(intn)//透过平方来寻找高度{for(inti=1;i<=n;i++){if(pow(i,2)>n){returni-1;}if(pow(i,2)==n){returni;}}return0;}voidgoldprint(inthigh,charc)//打印金字塔{intj;for(inti=1;i<=high;i++){for(j=0;j<high-i;j++)printf("");for(j=0;j<(2*i-1);j++)printf("%c",c);printf("\n");}}intmain(){intn,high;charc;scanf("%d",&n);getchar();c=getchar();high=clatulate(n);goldprint(high,c);printf("%d",(int)(n-pow(high,2)));}